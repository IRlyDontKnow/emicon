<table class="table table-sm">
    <thead>
    <tr>
        <th scope="col">Key</th>
        <th scope="col">Value</th>
    </tr>
    </thead>
    <tbody>
    {% for key, value in parameters %}
        <tr>
            <td>{{ key }}</td>
            <td>{{ value }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>
